#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/sherl/Documents/spaces/collab/CleanInfinity/server/node_modules/touch/bin/node_modules:/mnt/c/Users/sherl/Documents/spaces/collab/CleanInfinity/server/node_modules/touch/node_modules:/mnt/c/Users/sherl/Documents/spaces/collab/CleanInfinity/server/node_modules:/mnt/c/Users/sherl/Documents/spaces/collab/CleanInfinity/node_modules:/mnt/c/Users/sherl/Documents/spaces/collab/node_modules:/mnt/c/Users/sherl/Documents/spaces/node_modules:/mnt/c/Users/sherl/Documents/node_modules:/mnt/c/Users/sherl/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/sherl/Documents/spaces/collab/CleanInfinity/server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/sherl/Documents/spaces/collab/CleanInfinity/server/node_modules/touch/bin/node_modules:/mnt/c/Users/sherl/Documents/spaces/collab/CleanInfinity/server/node_modules/touch/node_modules:/mnt/c/Users/sherl/Documents/spaces/collab/CleanInfinity/server/node_modules:/mnt/c/Users/sherl/Documents/spaces/collab/CleanInfinity/node_modules:/mnt/c/Users/sherl/Documents/spaces/collab/node_modules:/mnt/c/Users/sherl/Documents/spaces/node_modules:/mnt/c/Users/sherl/Documents/node_modules:/mnt/c/Users/sherl/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/sherl/Documents/spaces/collab/CleanInfinity/server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../touch/bin/nodetouch.js" "$@"
else
  exec node  "$basedir/../touch/bin/nodetouch.js" "$@"
fi
